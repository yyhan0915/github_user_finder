{
    "name": "github_user_finder",
    "version": "0.2.0",
    "license": "MIT",
    "scripts": {
        "start": "cross-env NODE_ENV=development parcel --port 3000 index.html",
        "build": "cross-env NODE_ENV=production parcel build index.html",
        "lint": "npm run lint:scripts && npm run lint:declarations",
        "lint:scripts": "tslint './src/**/*.{ts,tsx}' --project tsconfig.json --fix --format stylish",
        "lint:declarations": "tslint --project tsconfig.json --format stylish --fix './typings/**/*'",
        "prepush": "npm run lint && tsc --noEmit",
        "dependencies:update": "npx npm-check --update --save-exact",
        "tsc": "tsc",
        "test": "jest"
    },
    "engines": {
        "node": ">=10",
        "npm": ">=6"
    },
    "dependencies": {
        "axios": "0.19.2",
        "bootstrap": "4.5.0",
        "react": "16.13.1",
        "react-bootstrap": "1.0.1",
        "react-dom": "16.13.1",
        "react-helmet-async": "1.0.6",
        "react-redux": "7.2.0",
        "react-router-dom": "5.2.0",
        "react-spinners": "0.8.3",
        "redux": "4.0.5",
        "redux-actions": "2.6.5",
        "redux-devtools-extension": "2.13.8",
        "redux-logger": "3.0.6",
        "redux-persist": "6.0.0",
        "redux-saga": "1.1.3",
        "redux-thunk": "2.3.0",
        "src": "1.1.2",
        "styled-components": "5.1.1",
        "typescript": "3.8.3"
    },
    "postcss": {
        "modules": true,
        "plugins": {
            "autoprefixer": {
                "browsers": [
                    ">5%"
                ],
                "flexbox": "no-2009"
            }
        }
    },
    "devDependencies": {
        "@types/enzyme": "3.10.5",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/jest": "25.2.3",
        "@types/react": "16.9.34",
        "@types/react-dom": "16.9.6",
        "@types/react-redux": "7.1.7",
        "@types/react-router-dom": "5.1.5",
        "@types/react-test-renderer": "16.9.2",
        "@types/redux-actions": "2.6.1",
        "@types/redux-logger": "3.0.8",
        "@types/styled-components": "5.1.0",
        "@types/webpack-env": "1.15.1",
        "autoprefixer": "9.3.1",
        "cross-env": "5.2.0",
        "custom-tslint-rules": "3.0.0",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "enzyme-to-json": "3.5.0",
        "husky": "1.2.0",
        "jest": "26.0.1",
        "parcel-bundler": "1.12.4",
        "postcss-modules": "2.0.0",
        "react-test-renderer": "16.13.1",
        "ts-jest": "26.1.0",
        "tslint": "5.11.0",
        "tslint-clean-code": "0.2.9",
        "tslint-consistent-codestyle": "1.14.1",
        "tslint-eslint-rules": "5.4.0",
        "tslint-immutable": "5.0.0",
        "tslint-microsoft-contrib": "5.2.1",
        "tslint-misc-rules": "3.5.1",
        "tslint-react": "3.6.0",
        "tslint-sonarts": "1.8.0",
        "vrsource-tslint-rules": "5.8.2"
    }
}
